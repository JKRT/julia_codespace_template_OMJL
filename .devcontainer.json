{
	"name": "Julia 1.10 Codespace",
	"image": "julia:1.10",
	"features": {
	  "ghcr.io/devcontainers/features/common-utils:2": {}
	},
	"postCreateCommand": "julia -e 'using Pkg; Pkg.instantiate(); include(setup.jl)'",
	"customizations": {
	  "vscode": {
		"extensions": [
		  "julialang.language-julia",
		  "ms-toolsai.jupyter"
		]
	  }
	},
	"remoteUser": "vscode"
  }
  
